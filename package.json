{
  "name": "help-wanted-action",
  "version": "0.1.1",
  "private": true,
  "description": "Assigns a help-wanted tag when there isn't anybody assigned to an issue.",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "pack": "ncc build",
    "test": "jest --passWithNoTests",
    "all": "yarn build && yarn format && yarn lint && yarn run pack && yarn test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mkqavi/help-wanted-action.git"
  },
  "keywords": [
    "actions"
  ],
  "author": "Marvin KÃ¶nig <dev@mkqavi.com>",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.2",
    "@actions/github": "^2.1.1"
  },
  "devDependencies": {
    "@types/jest": "^25.1.3",
    "@types/node": "^13.7.4",
    "@typescript-eslint/parser": "^2.8.0",
    "@zeit/ncc": "^0.21.1",
    "eslint": "^6.8.0",
    "eslint-plugin-github": "^3.4.1",
    "eslint-plugin-jest": "^23.8.0",
    "jest": "^25.1.0",
    "jest-circus": "^25.1.0",
    "js-yaml": "^3.13.1",
    "prettier": "^1.19.1",
    "ts-jest": "^25.2.1",
    "typescript": "^3.6.4"
  }
